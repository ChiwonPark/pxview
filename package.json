{
  "name": "PxView",
  "version": "1.0.0",
  "description": "Unofficial Pixiv app client for Android and iOS, built in React Native",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "lint": "eslint src --fix",
    "test": "jest",
    "test:watch": "jest --watch --coverage",
    "android": "node node_modules/react-native/local-cli/cli.js run-android",
    "android-release": "react-native run-android --variant=release",
    "android-clean": "cd android && ./gradlew clean",
    "ios": "react-native run-ios",
    "ios-release": "react-native run-ios --configuration Release",
    "pod-install": "cd ios && pod install"
  },
  "dependencies": {
    "bluebird": "^3.5.0",
    "firebase": "^4.1.3",
    "hoist-non-react-statics": "^2.0.0",
    "lodash.camelcase": "^4.3.0",
    "lodash.truncate": "^4.4.2",
    "moment": "^2.18.1",
    "normalizr": "^3.2.3",
    "pixiv-api-client": "^0.14.0",
    "prop-types": "^15.5.10",
    "qs": "^6.5.0",
    "react": "16.0.0-alpha.12",
    "react-native": "^0.45.1",
    "react-native-action-button": "^2.7.1",
    "react-native-animatable": "^1.2.2",
    "react-native-blur": "^3.1.2",
    "react-native-cookies": "^3.1.0",
    "react-native-datepicker": "^1.6.0",
    "react-native-device-info": "^0.10.2",
    "react-native-easy-toast": "^1.0.6",
    "react-native-elements": "^0.12.2",
    "react-native-fabric": "^0.4.1",
    "react-native-fetch-blob": "^0.10.6",
    "react-native-htmlview": "^0.10.0",
    "react-native-hyperlink": "0.0.9",
    "react-native-loading-spinner-overlay": "^0.5.0",
    "react-native-localization": "^0.1.30",
    "react-native-material-bottom-navigation": "^0.5.1",
    "react-native-material-kit": "^0.4.1",
    "react-native-message-bar": "^1.6.0",
    "react-native-open-settings": "0.0.3",
    "react-native-photo-view": "^1.3.0",
    "react-native-progress": "^3.3.0",
    "react-native-share": "^1.0.20",
    "react-native-spinkit": "^1.0.3",
    "react-native-splash-screen": "^2.1.0",
    "react-native-tab-view": "0.0.66",
    "react-native-vector-icons": "^4.2.0",
    "react-navigation": "^1.0.0-beta.11",
    "react-redux": "^5.0.5",
    "redux": "^3.7.1",
    "redux-action-buffer": "^1.1.0",
    "redux-define": "^1.1.1",
    "redux-enhancer-react-native-appstate": "^0.2.0",
    "redux-form": "^6.8.0",
    "redux-persist": "^4.8.2",
    "redux-saga": "^0.15.4",
    "reselect": "^3.0.1",
    "shallow-equals": "^1.0.0"
  },
  "jest": {
    "preset": "react-native",
    "transformIgnorePatterns": [
      "node_modules/(?!react-native|redux-enhancer-react-native-appstate|)"
    ]
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react-native": "2.0.0",
    "babel-register": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-config-prettier": "^2.3.0",
    "eslint-plugin-import": "^2.6.1",
    "eslint-plugin-jest": "^20.0.3",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-prettier": "^2.1.2",
    "eslint-plugin-react": "^7.1.0",
    "eslint-plugin-react-native": "^2.3.2",
    "jest": "^20.0.4",
    "prettier": "^1.5.2",
    "prettier-eslint": "^6.4.1",
    "react-test-renderer": "15.6.1",
    "redux-immutable-state-invariant": "^2.0.0"
  }
}
